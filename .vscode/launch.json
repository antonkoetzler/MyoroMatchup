{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Flutter",
            "request": "launch",
            "type": "dart",
            "program": "myoro_matchup/lib/main.dart",
            "args": [
                "--dart-define=env=${input:environment}"
            ]
        },
        {
            "type": "java",
            "name": "Spring Boot-MyoroMatchupApiApplication<myoro-matchup-api>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.myoro.myoro_matchup_api.MyoroMatchupApiApplication",
            "projectName": "myoro-matchup-api",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "preLaunchTask": "Start PostgreSQL container"
        },
    ],
    "inputs": [
        {
            "id": "environment",
            "description": "Select environment",
            "type": "pickString",
            "options": [
                "local",
                "dev",
                "prod"
            ],
            "default": "local"
        }
    ]
}